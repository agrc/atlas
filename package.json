    "cpy-cli": "^5.0.0",
    "eslint": "^9.19.0",
    "postcss": "^8.5.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.1.6",
