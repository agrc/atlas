    "@ugrc/utah-design-system": "^1.20.2",
    "@ugrc/utilities": "^3.0.2",
    "firebase": "^11.0.1",
    "ky": "^1.7.4",
    "lodash.startcase": "^4.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-aria": "^3.37.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^18.3.1",
